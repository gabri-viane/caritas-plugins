            <HBox>
               <children>
                  <BorderPane styleClass="anchor-pane-shadowed">
                     <center>
                        <VBox alignment="CENTER" spacing="5.0" BorderPane.alignment="CENTER">
                           <children>
                              <HBox alignment="BOTTOM_CENTER" VBox.vgrow="NEVER">
                                 <children>
                                    <Label fx:id="nameLB" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Nome" wrapText="true" HBox.hgrow="ALWAYS">
                                       <font>
                                          <Font size="14.0" />
                                       </font>
                                    </Label>
                                 </children>
                              </HBox>
                              <HBox alignment="TOP_CENTER" VBox.vgrow="ALWAYS">
                                 <children>
                                    <Label fx:id="dimensionLB" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Dimensione" wrapText="true" HBox.hgrow="ALWAYS">
