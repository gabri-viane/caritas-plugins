                           <children>
                              <CheckBox fx:id="insertCB" mnemonicParsing="false" text="Inserisci" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                     </bottom>
                  </BorderPane>
                  <Separator orientation="VERTICAL" />
               </children>
            </HBox>
         </children>
      </StackPane>
   </children>
</fx:root>
