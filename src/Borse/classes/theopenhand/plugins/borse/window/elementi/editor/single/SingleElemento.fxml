<fx:root maxHeight="95.0" maxWidth="200.0" minHeight="95.0" minWidth="200.0" prefHeight="95.0" prefWidth="200.0" type="AnchorPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
